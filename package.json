		"discord.js": "^12.3.0",
		"dotenv": "^8.2.0",
		"ffmpeg": "0.0.4",
		"fluent-ffmpeg": "^2.1.2",
		"opusscript": "0.0.7",
		"setimmediate": "^1.0.5",
		"simple-youtube-api": "^5.2.1",
		"sqlite3": "^4.2.0",
		"ytdl-core": "^2.1.7",
		"ytdl-core-discord": "^1.2.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.11.1",
		"@babel/node": "^7.10.5",
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
